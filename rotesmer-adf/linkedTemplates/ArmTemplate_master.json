{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "rotesmer-adf"
		},
		"SQLDB_100M_DB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SQLDB_100M_DB'",
			"defaultValue": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=fox-au-east.database.windows.net;Initial Catalog=sqldb-100m-rows;User ID=rotesmer-admin"
		},
		"SQLDB_AdventureWorks_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SQLDB_AdventureWorks'"
		},
		"adfakv_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://rotesmer-adf-akv.vault.azure.net/"
		},
		"foxaueast_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://foxaueast.dfs.core.windows.net/"
		},
		"default_properties_env_value": {
			"type": "string",
			"defaultValue": "dev"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "rotesmer-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"SQLDB_AdventureWorks_connectionString": {
						"value": "[parameters('SQLDB_AdventureWorks_connectionString')]"
					},
					"SQLDB_100M_DB_connectionString": {
						"value": "[parameters('SQLDB_100M_DB_connectionString')]"
					},
					"default_properties_env_value": {
						"value": "[parameters('default_properties_env_value')]"
					},
					"adfakv_properties_typeProperties_baseUrl": {
						"value": "[parameters('adfakv_properties_typeProperties_baseUrl')]"
					},
					"foxaueast_properties_typeProperties_url": {
						"value": "[parameters('foxaueast_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}