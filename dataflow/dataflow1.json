{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "csvparkingviolations",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"name": "sink1"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          {Summons Number} as string,",
				"          {Plate ID} as string,",
				"          {Registration State} as string,",
				"          {Plate Type} as string,",
				"          {Issue Date} as string,",
				"          {Violation Code} as string,",
				"          {Vehicle Body Type} as string,",
				"          {Vehicle Make} as string,",
				"          {Issuing Agency} as string,",
				"          {Street Code1} as string,",
				"          {Street Code2} as string,",
				"          {Street Code3} as string,",
				"          {Vehicle Expiration Date} as string,",
				"          {Violation Location} as string,",
				"          {Violation Precinct} as string,",
				"          {Issuer Precinct} as string,",
				"          {Issuer Code} as string,",
				"          {Issuer Command} as string,",
				"          {Issuer Squad} as string,",
				"          {Violation Time} as string,",
				"          {Time First Observed} as string,",
				"          {Violation County} as string,",
				"          {Violation In Front Of Or Opposite} as string,",
				"          {House Number} as string,",
				"          {Street Name} as string,",
				"          {Intersecting Street} as string,",
				"          {Date First Observed} as string,",
				"          {Law Section} as string,",
				"          {Sub Division} as string,",
				"          {Violation Legal Code} as string,",
				"          {Days Parking In Effect    } as string,",
				"          {From Hours In Effect} as string,",
				"          {To Hours In Effect} as string,",
				"          {Vehicle Color} as string,",
				"          {Unregistered Vehicle?} as string,",
				"          {Vehicle Year} as string,",
				"          {Meter Number} as string,",
				"          {Feet From Curb} as string,",
				"          {Violation Post Code} as string,",
				"          {Violation Description} as string,",
				"          {No Standing or Stopping Violation} as string,",
				"          {Hydrant Violation} as string,",
				"          {Double Parking Violation} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     format: 'table') ~> sink1"
			]
		}
	}
}